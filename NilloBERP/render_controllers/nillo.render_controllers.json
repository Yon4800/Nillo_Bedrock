{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.nillo": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.default",
            "Texture.aqua",
            "Texture.earth",
            "Texture.fire",
            "Texture.wind"
          ]
        },
        "geometries": {
          "Array.geo":[
            "geometry.default",
            "geometry.aqua",
            "geometry.earth",
            "geometry.fire",
            "geometry.wind"
          ]
        }
      },
      "geometry": "Array.geo[query.skin_id]",
      "materials": [
        { "*": "Material.default" }
      ],
      "textures": [
        "Array.skins[query.skin_id]"
      ]
    }
  }
}
